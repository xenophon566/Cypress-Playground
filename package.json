{
    "name": "cypress-playground",
    "version": "1.0.0",
    "scripts": {
        "start": "ng serve",
        "cy:play-r": "npx cypress run --config-file cy-playground.json",
        "cy:play-o": "npx cypress open --config-file cy-playground.json",
        "cy:ex-r": "npx cypress run --config-file cy-example.json",
        "cy:ex-o": "npx cypress open --config-file cy-example.json",
        "cy:rwd-o": "npx cypress open --config-file cy-rwd.json",
        "cy:ua": "npm-run-all test:user-r test:agent-r",
        "cy:ua-r": "npm-run-all -p test:user-r test:agent-r",
        "cy:ua-o": "npm-run-all -p test:user-o test:agent-o",
        "test:user-r": "npx cypress run --config-file cy-user.json",
        "test:user-o": "npx cypress open --config-file cy-user.json",
        "test:agent-r": "npx cypress run --config-file cy-agent.json",
        "test:agent-o": "npx cypress open --config-file cy-agent.json",
        "cy:run": "npx cypress run",
        "cy:open": "npx cypress open",
        "ng": "ng",
        "build": "ng build"
    },
    "dependencies": {
        "@angular/animations": "^14.0.0",
        "@angular/common": "^14.0.0",
        "@angular/compiler": "^14.0.0",
        "@angular/core": "^14.0.0",
        "@angular/forms": "^14.0.0",
        "@angular/platform-browser": "^14.0.0",
        "@angular/platform-browser-dynamic": "^14.0.0",
        "@angular/router": "^14.0.0",
        "font-awesome": "^4.7.0",
        "rxjs": "~7.5.0",
        "tslib": "^2.3.0",
        "zone.js": "~0.11.4"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^14.0.4",
        "@angular/cli": "~14.0.4",
        "@angular/compiler-cli": "^14.0.0",
        "@types/jasmine": "~4.0.0",
        "cypress": "^9.5.2",
        "cypress-iframe": "^1.0.1",
        "cypress-recurse": "^1.21.0",
        "cypress-wait-until": "^1.7.2",
        "cypress-xpath": "^1.6.2",
        "jasmine-core": "~4.1.0",
        "karma": "~6.3.0",
        "karma-chrome-launcher": "~3.1.0",
        "karma-coverage": "~2.2.0",
        "karma-jasmine": "~5.0.0",
        "karma-jasmine-html-reporter": "~1.7.0",
        "npm-run-all": "^4.1.5",
        "typescript": "~4.7.2"
    }
}
